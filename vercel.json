{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/pdf",
      "dest": "/api/pdf.js"
    },
    {
      "src": "/(.*).html",
      "dest": "/$1.html"
    }
  ]
}
